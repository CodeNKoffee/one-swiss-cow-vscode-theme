{
  "name": "One Swiss Cow",
  "type": "dark",
  "colors": {
    // Primary UI colors inspired by Swiss pastoral landscapes
    "editor.background": "#2D3047",          // Deep twilight blue of Alpine evenings
    "editor.foreground": "#E8E9F3",          // Fresh milk white
    "editor.lineHighlightBackground": "#363B5E", // Subtle highlight
    "editor.selectionBackground": "#4A5089",  // Mountain shadow
    "editor.inactiveSelectionBackground": "#3A4069",
    
    // Sidebar and activity bar
    "activityBar.background": "#222537",     // Darker evening sky
    "activityBar.foreground": "#B7B8C4",     // Muted milk white
    "activityBar.inactiveForeground": "#6B6D7B",
    "activityBarBadge.background": "#BE4F4F", // Swiss flag red
    "activityBarBadge.foreground": "#E8E9F3",
    
    // Sidebar
    "sideBar.background": "#252A3D",         // Alpine shadow
    "sideBar.foreground": "#B7B8C4",
    "sideBarTitle.foreground": "#E8E9F3",
    "sideBarSectionHeader.background": "#2D3047",
    "sideBarSectionHeader.foreground": "#B7B8C4",
    
    // Title bar
    "titleBar.activeBackground": "#222537",
    "titleBar.activeForeground": "#E8E9F3",
    "titleBar.inactiveBackground": "#1E2130",
    "titleBar.inactiveForeground": "#6B6D7B",
    
    // Tabs
    "tab.activeBackground": "#2D3047",
    "tab.inactiveBackground": "#252A3D",
    "tab.activeForeground": "#E8E9F3",
    "tab.inactiveForeground": "#8F90A0",
    "tab.activeBorderTop": "#BE4F4F",        // Swiss flag red accent
    
    // Status bar
    "statusBar.background": "#222537",
    "statusBar.foreground": "#B7B8C4",
    "statusBar.debuggingBackground": "#BE4F4F",
    
    // Input and panels
    "input.background": "#363B5E",
    "input.foreground": "#E8E9F3",
    "panel.background": "#252A3D",
    "panelTitle.activeForeground": "#E8E9F3"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7B8096",            // Misty mountain gray
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#DCE0F2"             // Bright milk white
      }
    },
    {
      "name": "Keywords and Control",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#BE4F4F"             // Swiss flag red
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted.double.html",
        "string.quoted.single.html"
      ],
      "settings": {
        "foreground": "#90B06E"             // Alpine meadow green
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#7B95B7"             // Alpine lake blue
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#D4A256"             // Golden brown like cow bell
      }
    },
    {
      "name": "Numbers and Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#C58F7D"             // Swiss brown cow color
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml"
      ],
      "settings": {
        "foreground": "#BE4F4F"             // Swiss flag red
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#D4A256",            // Golden brown
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Headings",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#BE4F4F",            // Swiss flag red
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#D4A256",            // Golden brown
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#7B95B7",            // Alpine lake blue
        "fontStyle": "italic"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "parameter": "#B7B8C4",                 // Muted milk white
    "property": "#A3A4B8",                  // Slightly darker milk white
    "type": "#D4A256",                      // Golden brown
    "class": "#D4A256",                     // Golden brown
    "enum": "#BE4F4F",                      // Swiss flag red
    "interface": "#7B95B7",                 // Alpine lake blue
    "typeParameter": "#C58F7D",             // Swiss brown
    "function": "#7B95B7",                  // Alpine lake blue
    "method": "#7B95B7",                    // Alpine lake blue
    "decorator": "#BE4F4F",                 // Swiss flag red
    "regex": "#90B06E"                      // Alpine meadow green
  }
}